<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="5dp"
                android:color="#3560FD" />
            <solid
                android:color="#00000000" />
            <corners
                android:radius="15dp" />
        </shape>
    </item>

    <item
        android:top="15dp" android:bottom="15dp"
        android:right="15dp" android:left="15dp">
        <shape
            android:shape="oval">
            <stroke
                android:width="2dp"
                android:color="#FF55DA" />
            <solid android:color="#00000000" />
        </shape>
    </item>
</layer-list>
